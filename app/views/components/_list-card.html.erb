
<% if list.image.key %>
  <div class="list-card_container" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(<%= cl_image_path list.image.key, height: 300, width:400, crop: :fill %>)">
    <%= list.name  %>
  </div>
<% else %>
  <div class="list-card_container">
    <%= list.name  %>
  </div>
<% end %>

